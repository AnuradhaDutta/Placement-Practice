import java.util.*;
class Main
{
    static void Duplicate(String str)
    {
        char arr[]=str.toCharArray();
        Arrays.sort(arr);
        int i=0;
        while(i<arr.length-1)
        {
            if(arr[i]==arr[i+1])
            {
                System.out.print(arr[i]+"");
            }    
        i++;
    }
    }
    public static void main(String []args)
    {
        String str="aabcdeeffggh";
        Duplicate(str);
    }
}
